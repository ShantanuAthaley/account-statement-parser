{
  "iciciBankSearchStatementConfig": {
    "version": "1.0",
    "description": "Generic bank account statement parser configuration",
    "sections": [
      {
        "id": "header",
        "order": 1,
        "name": "header_title",
        "title": "Detailed Statement",
        "searchKeywords": [
          "DETAILED STATEMENT"
        ],
        "relativeSearchRange": {
          "rows": 5,
          "columns": 5
        }
      },
      {
        "id": "search_criteria",
        "order": 2,
        "title": "Search Section",
        "searchKeywords": [
          "Search"
        ],
        "fields": [
          {
            "name": "account_number",
            "label": "Account Number",
            "pattern": "\\d{12}",
            "valueAfterText": true
          },
          {
            "name": "transaction_date_from",
            "label": "Transaction Date from",
            "pattern": "\\d{2}/\\d{2}/\\d{4}",
            "valueAfterText": true
          },
          {
            "name": "transaction_date_to",
            "label": "Transaction Date to",
            "pattern": "\\d{2}/\\d{2}/\\d{4}",
            "valueAfterText": true
          }
        ]
      },
      {
        "id": "advance_search",
        "order": 2,
        "title": "Advanced Search",
        "searchKeywords": [
          "Advanced Search"
        ],
        "fields": [
          {
            "name": "amount_from",
            "label": "Amount from",
            "pattern": "\\d{12}",
            "valueAfterText": true
          },
          {
            "name": "transaction_date_from",
            "label": "Transaction Date from",
            "pattern": "\\d{2}/\\d{2}/\\d{4}",
            "valueAfterText": true
          },
          {
            "name": "transaction_date_to",
            "label": "Transaction Date to",
            "pattern": "\\d{2}/\\d{2}/\\d{4}",
            "valueAfterText": true
          }
        ]
      },
      {
        "id": "transactions_table",
        "order": 3,
        "title": "Transactions List",
        "searchKeywords": [
          "Transactions List"
        ],
        "relativeSearchRange": {
          "rows": 10,
          "columns": 10
        },
        "table": {
          "headerRowOffset": 1,
          "columns": [
            "S No.",
            "Value Date",
            "Transaction Date",
            "Cheque Number",
            "Transaction Remarks",
            "Withdrawal Amount(INR)",
            "Deposit Amount(INR)",
            "Balance(INR)"
          ]
        }
      }
    ]
  }
}
